<h1>Editing spotify_track</h1>

<% form_for(@spotify_track) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :duration %><br />
    <%= f.text_field :duration %>
  </p>
  <p>
    <%= f.label :track_number %><br />
    <%= f.text_field :track_number %>
  </p>
  <p>
    <%= f.label :disc_number %><br />
    <%= f.text_field :disc_number %>
  </p>
  <p>
    <%= f.label :preview_url %><br />
    <%= f.text_field :preview_url %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @spotify_track %> |
<%= link_to 'Back', spotify_tracks_path %>